{
  "type": "module",
  "private": true,
  "scripts": {
    "build": "dotenv -e ./.env.production.local -v NODE_ENV=production -- nuxt build",
    "depcheck": "depcheck",
    "dev": "dotenv -e ./.env.development.local -v NODE_ENV=development -- nuxt dev",
    "generate": "dotenv -e ./.env.production.local -v NODE_ENV=production -- nuxt generate",
    "lint": "eslint --max-warnings 0 && stylelint './app/**/*.{css,scss,vue}' -i ./.gitignore --mw 0",
    "lint:fix": "eslint --fix --max-warnings 0 && stylelint './app/**/*.{css,scss,vue}' -i ./.gitignore --fix --mw 0",
    "postinstall": "nuxt prepare",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "unused-exports": "ts-unused-exports ./tsconfig.json"
  },
  "dependencies": {
    "@kikiutils/nuxt": "^9.0.0",
    "@kikiutils/shared": "^13.5.2",
    "@nuxtjs/robots": "^5.5.5",
    "@vueuse/nuxt": "^13.9.0",
    "nuxt": "^4.1.3",
    "nuxt-security": "^2.4.0"
  },
  "devDependencies": {
    "@kikiutils/eslint-config": "^4.0.0",
    "@kikiutils/stylelint-config": "^0.2.0",
    "@unocss/nuxt": "^66.5.4",
    "depcheck": "^1.4.7",
    "dotenv-cli": "^10.0.0",
    "postcss-pxtorem": "^6.1.0",
    "ts-unused-exports": "^11.0.1",
    "unocss": "^66.5.4",
    "unplugin-fonts": "^1.4.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  }
}
